@model IEnumerable<Page>


@foreach (var item in Model)
{
    if (item.Slug != "home")
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-route-slug="@item.Slug" asp-controller="Pages" asp-action="Page">@item.Title</a>
        </li>
    }
}

<li class="nav-item">
    <a class="nav-link text-dark" asp-area="" asp-controller="Products" asp-action="Index">Products</a>
</li>